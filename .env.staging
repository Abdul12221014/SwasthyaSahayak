# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_URL=your_supabase_url_here

# AI/ML Configuration
LOVABLE_API_KEY=your_lovable_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_key_here
HUGGINGFACE_API_KEY=your_hf_key_here

# ML Inference Service
ML_SERVICE_URL=http://localhost:8000
ML_SERVICE_PORT=8000
MODEL_VERSION_TRACKING=true

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_sid_here
TWILIO_AUTH_TOKEN=your_twilio_token_here
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886
TWILIO_PHONE_NUMBER=your_twilio_phone_here

# ML Model Paths (for inference service)
EMBEDDING_MODEL_PATH=./src/ml/models/embeddings/model_v1
EMERGENCY_CLASSIFIER_PATH=./src/ml/models/emergency/model_v1
TRANSLATION_MODEL_PATH=./src/ml/models/translation/model_v1

# Database
DATABASE_URL=postgresql://user:pass@localhost:5432/swasthya
PGVECTOR_DIMENSION=768

# RAG Configuration
RAG_TOP_K=5
RAG_SIMILARITY_THRESHOLD=0.7
RAG_RERANK_ENABLED=true

# Environment
NODE_ENV=development
PYTHON_ENV=development

# Admin & Security
ADMIN_INGEST_TOKEN=change-me-in-production
VITE_ADMIN_TOKEN=change-me-in-production

# Rate Limiting
RATE_LIMIT_WINDOW=600000
RATE_LIMIT_MAX=20

# Cache Configuration
CACHE_EMBEDDING_TTL=1800000
CACHE_RETRIEVAL_TTL=600000

# API Endpoints (for frontend)
VITE_INGEST_API_URL=https://your-project.supabase.co/functions/v1/ingest-documents
VITE_REEMBED_API_URL=https://your-project.supabase.co/functions/v1/reembed-kb
VITE_ML_SERVICE_URL=http://localhost:8000

# Feature Flags
FEATURE_VACCINATION_API=true
FEATURE_OUTBREAK_API=true
FEATURE_PHC_LOOKUP=true

# Government API Integration
ODISHA_HEALTH_API_BASE=https://api.odisha-health.example
ODISHA_HEALTH_API_KEY=change-me
PHC_FACILITIES_CSV=src/ml/data/processed/odisha_phc_directory.csv

# Geo Location
GEOIP_PROVIDER=none
DEFAULT_DISTRICT=Cuttack

# Environment & Deployment
NODE_ENV=development
ENV_NAME=development   # development|staging|production

# Observability
LOG_LEVEL=info
OBS_ENABLE_METRICS=true
OBS_ENABLE_REQUEST_IDS=true
ENABLE_REQUEST_LOGGING=true

# Security
ALLOWLIST_OUTBOUND_HOSTS=who.int,mohfw.gov.in,unicef.org,icmr.nic.in,nhp.gov.in,ayush.gov.in,cdc.gov

# Alert System
ALERT_MAX_USERS_PER_RUN=1000

# Health Checks
HEALTH_CHECK_TIMEOUT_MS=5000

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

